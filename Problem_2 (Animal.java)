
interface Jump {
	  public void jump(); 
}

interface Bark {
	  public void bark(); 
}

interface Croak {
	  public void croack(); 
}

public abstract class Animal implements Jump{
	private String name;
	
	public void setName(String name){
		this.name = name;
	}
	
	public String getName(){
		return name;
	}
	
	public void jump() {
		  System.out.print("I jump!");
	}	
}
